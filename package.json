        "@wdio/spec-reporter": "^7.12.5",
        "@wdio/sync": "^7.12.5",
        "@wdio/types": "^7.12.5",
        "chromedriver": "^93.0.1",
        "del-cli": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "2.4.1",
